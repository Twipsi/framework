{
  "name": "twipsi/framework",
  "description": "The Twipsi PHP MVC Framework",
  "keywords": ["twipsi","mvc","framework"],
  "homepage": "http://www.github.com/twipsi/framework",
  "license": "MIT",
  "authors": [
      {
          "name": "Petrik GÃ¡bor",
          "email": "admin@twipsi.com"
      }
  ],
  "require": {
    "php": "^8.1.0",
    "ext-pdo": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "tijsverkoyen/css-to-inline-styles": "^2.2",
    "symfony/mailer": "^6.1",
    "symfony/mailgun-mailer": "^6.1",
    "symfony/mailchimp-mailer": "^6.1",
    "symfony/google-mailer": "^6.1",
    "symfony/error-handler": "^6.1",
    "filp/whoops": "^2.14",
    "opis/closure": "^3.6",
    "symfony/console": "^6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpstan/phpstan": "^1.4"
  },
  "autoload": {
      "files": [
          "src/Twipsi/Helpers/App.php",
          "src/Twipsi/Helpers/Asset.php",
          "src/Twipsi/Helpers/Auth.php"
      ],
      "psr-4": {
          "Twipsi\\": "src/Twipsi/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Twipsi\\Tests\\": "tests/"
      }
  },
  "config": {
      "sort-packages": true,
      "allow-plugins": {
          "composer/package-versions-deprecated": true
      }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
